{
  "version": 2,
  "builds": [
    {
      "src": "src/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/src/static/assets/$1"
    },
    {
      "src": "/t/(.*)",
      "dest": "/src/wsgi.py"
    },
    {
      "src": "/p/(.*)",
      "dest": "/src/wsgi.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/wsgi.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_DEBUG": "False",
    "PYTHONPATH": "/var/task/src"
  }
}

